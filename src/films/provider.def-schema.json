{
  "type": "object",
  "required": ["name", "type", "url"],
  "additionalProperties": false,
  "properties": {
    "address": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "chain": {
      "type": "string"
    },
    "email": {
      "type": "string",
      "format": "email"
    },
    "phone": {
      "type": "string",
      "pattern": "^[0-9 +]+$"
    },
    "ref": {
      "type": "string"
    },
    "type": {
      "pattern": "cinema|live-tv|streaming",
      "type": "string"
    },
    "url": {
      "type": "string",
      "format": "uri"
    }
  }
}
