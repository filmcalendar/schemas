{
  "$id": "http://filmcalendar.schemas/cinemas/schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["entities", "result"],
  "additionalProperties": false,
  "properties": {
    "entities": {
      "type": "object",
      "required": ["venues", "films", "sessions", "pages"],
      "additionalProperties": false,
      "properties": {
        "venues": {
          "type": "object",
          "minProperties": 1,
          "propertyNames": {
            "pattern": "^[a-z0-9-]*$"
          },
          "additionalProperties": {
            "$ref": "#/definitions/venue"
          }
        },
        "films": {
          "minProperties": 1,
          "additionalProperties": {
            "$ref": "#/definitions/film"
          },
          "propertyNames": {
            "pattern": "^[a-z0-9-]*$"
          }
        },
        "sessions": {
          "minProperties": 1,
          "type": "object",
          "additionalProperties": {
            "$ref": "#/definitions/session"
          },
          "propertyNames": {
            "pattern": "^[a-z0-9-]*$"
          }
        },
        "pages": {
          "minProperties": 1,
          "type": "object",
          "additionalProperties": {
            "$ref": "#/definitions/page"
          },
          "propertyNames": {
            "pattern": "^[a-z0-9]*$"
          }
        },
        "sessionAttributes": {
          "type": "object",
          "additionalProperties": {
            "$ref": "#/definitions/sessionAttribute"
          },
          "propertyNames": {
            "pattern": "^[a-z0-9-]*$"
          }
        }
      }
    },
    "result": {
      "type": "array",
      "minItems": 1,
      "uniqueItems": true,
      "items": {
        "type": "string"
      }
    }
  }
}
